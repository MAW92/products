function populateProductCard(productData) {
	// Select the product card template and clone it
	const productCardTemplate = document.querySelector('#product-card');
	const productCard = productCardTemplate.cloneNode(true);

	// Populate the card with product data
	productCard.querySelector('#viewItemURL').href = productData.viewItemURL;
	productCard.querySelector('#galleryURL').src = productData.galleryURL;
	productCard.querySelector('#galleryURL').alt = productData.title;
	productCard.querySelector('#title').textContent = productData.title;
	productCard.querySelector('#itemId').href = productData.itemId;
	productCard.querySelector('#condition').textContent = productData.condition;
	productCard.querySelector('#currentPrice').textContent = productData.currentPrice;

	// Append the card to the product container
	const productContainer = document.querySelector('#product-container');
	productContainer.appendChild(productCard);

	// Show the product card
	productCard.style.display = 'block';
}

// Loop through the product data array and populate the card with each product's data
productData.forEach((productData) => {
	populateProductCard(productData);
});
