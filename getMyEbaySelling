function getMyEbaySelling() {
  const xhr=new XMLHttpRequest();
  const url='https://api.ebay.com/ws/api.dll';
  const requestBody=`<?xml version="1.0" encoding="utf-8"?>
    <GetMyeBaySellingRequest xmlns="urn:ebay:apis:eBLBaseComponents">
      <RequesterCredentials>
      <eBayAuthToken>v^1.1#i^1#p^3#f^0#I^3#r^0#t^</eBayAuthToken>
      </RequesterCredentials>
      <ActiveList>
        <Sort>TimeLeft</Sort>
        <Pagination>
          <EntriesPerPage>100</EntriesPerPage>
          <PageNumber>2</PageNumber>
        </Pagination>
      </ActiveList>
    </GetMyeBaySellingRequest>`;

  xhr.open('POST',url,true);
  xhr.setRequestHeader('X-EBAY-API-SITEID','0');
  xhr.setRequestHeader('X-EBAY-API-COMPATIBILITY-LEVEL','967');
  xhr.setRequestHeader('X-EBAY-API-CALL-NAME','GetMyeBaySelling');
  xhr.setRequestHeader('X-EBAY-API-IAF-TOKEN','v^1.1#i^1#p^3#f^0#I^3#r^0#t^');
  xhr.setRequestHeader('Content-Type','text/xml');

  xhr.onreadystatechange=function () {
    if(xhr.readyState===XMLHttpRequest.DONE&&xhr.status===200) {
      // API call successful, handle response here
      const response=xhr.responseText;
      console.log(response);
    } else if(xhr.readyState===XMLHttpRequest.DONE&&xhr.status!==200) {
      // API call failed, handle error here
      const error=xhr.responseText;
      console.log(error);
    }
  };

  xhr.send(requestBody);
}
